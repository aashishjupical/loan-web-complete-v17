#. module: jt_loan_management
#: model:mail.template,body_html:jt_loan_management.mail_template_data_notification_email_loan_details
msgid ""
"<html>\n"
"               <![CDATA[<html>\n"
"                <head></head>\n"
"                % set record = ctx.get('record')\n"
"                % set company = record and record.company_id or ctx.get('company')\n"
"                <body style=\"margin: 0; padding: 0;\">\n"
"                <table border=\"0\" width=\"100%\" cellpadding=\"0\" bgcolor=\"#ededed\" style=\"padding: 20px; background-color: #ededed; border-collapse:separate;\" summary=\"o_mail_notification\">\n"
"                    <tbody>\n"
"\n"
"                      <!-- HEADER -->\n"
"                      <tr>\n"
"                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                          <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; background-color: rgb(135,90,123); padding: 20px; border-collapse:separate;\">\n"
"                            <tr>\n"
"                              <td valign=\"middle\">\n"
"                                  <span style=\"font-size:20px; color:white; font-weight: bold;\">\n"
"                                      ${object.record_name}\n"
"                                  </span>\n"
"                              </td>\n"
"                              <td valign=\"middle\" align=\"right\">\n"
"                                  <img src=\"/logo.png?company=${company.id}\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"${company.name}\">\n"
"                              </td>\n"
"                            </tr>\n"
"                          </table>\n"
"                        </td>\n"
"                      </tr>\n"
"\n"
"                      <!-- CONTENT -->\n"
"                      <tr>\n"
"                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                          <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#ffffff\" style=\"min-width: 590px; background-color: rgb(255, 255, 255); padding: 20px; border-collapse:separate;\">\n"
"                            <tbody>\n"
"                              <td valign=\"top\" style=\"font-family:Arial,Helvetica,sans-serif; color: #555; font-size: 14px;\">\n"
"                                ${object.body | safe}\n"
"                              </td>\n"
"                            </tbody>\n"
"                          </table>\n"
"                        </td>\n"
"                      </tr>\n"
"\n"
"                      <!-- FOOTER -->\n"
"                      <tr>\n"
"                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                          <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; background-color: rgb(135,90,123); padding: 20px; border-collapse:separate;\">\n"
"                            <tr>\n"
"                              <td valign=\"middle\" align=\"left\" style=\"color: #fff; padding-top: 10px; padding-bottom: 10px; font-size: 12px;\">\n"
"                                ${company.name}<br/>\n"
"                                ${company.phone or ''}\n"
"                              </td>\n"
"                              <td valign=\"middle\" align=\"right\" style=\"color: #fff; padding-top: 10px; padding-bottom: 10px; font-size: 12px;\">\n"
"                                % if company.email:\n"
"                                <a href=\"mailto:${company.email}\" style=\"text-decoration:none; color: white;\">${company.email}</a><br/>\n"
"                                % endif\n"
"                                % if company.website:\n"
"                                    <a href=\"${company.website}\" style=\"text-decoration:none; color: white;\">\n"
"                                        ${company.website}\n"
"                                    </a>\n"
"                                % endif\n"
"                              </td>\n"
"                            </tr>\n"
"                          </table>\n"
"                        </td>\n"
"                      </tr>\n"
"                      <tr>\n"
"                        <td align=\"center\">\n"
"                            Powered by <a target=\"_blank\" href=\"https://www.odoo.com\">Odoo</a>.\n"
"                        </td>\n"
"                      </tr>\n"
"                    </tbody>\n"
"                </table>\n"
"                </body>\n"
"                </html>\n"
"            "
msgstr ""
"<html>\n"
" <![CDATA[<html>\n"
" <cabeza></cabeza>\n"
" % establecer registro = ctx.get('registro')\n"
" % establecer empresa = registro y registro.company_id o ctx.get('empresa')\n"
" <estilo del cuerpo=\"margen: 0; relleno: 0;\">\n"
" <table border=\"0\" width=\"100%\" cellpadding=\"0\" bgcolor=\"#ededed\" style=\"padding: 20px; color de fondo: #ededed; border-collapse:separate;\" resumen=\"o_mail_notification\">\n"
" <tcuerpo>\n"
"\norte"
" <!-- ENCABEZADO -->\n"
"<tr>\n"
" <td align=\"center\" style=\"min-width: 590px;\">\n"
" <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; color de fondo: rgb(135,90,123); relleno: 20px; border-collapse:separate;\">\n"
"<tr>\n"
" <td valign=\"medio\">\n"
" <span style=\"font-size:20px; color blanco; peso de fuente: negrita;\">\n"
" ${object.record_name}\n"
" </span>\n"
"</td>\n"
" <td valign=\"medio\" align=\"derecha\">\n"
" <img src=\"/logo.png?company=${company.id}\" style=\"relleno: 0px; margen: 0px; altura: automático; ancho: 80px;\" alt=\"${empresa.nombre}\">\n"
"</td>\n"
"</tr>\n"
"</tabla>\n"
"</td>\n"
"</tr>\n"
"\norte"
" <!-- CONTENIDO -->\n"
"<tr>\n"
" <td align=\"center\" style=\"min-width: 590px;\">\n"
" <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#ffffff\" style=\"min-width: 590px; color de fondo: rgb (255, 255, 255); relleno: 20px; border-collapse:separate;\">\n"
" <tcuerpo>\n"
" <td valign=\"top\" style=\"font-family:Arial,Helvetica,sans-serif; color: #555; tamaño de fuente: 14px;\">\n"
" ${object.body | safe}\n"
"</td>\n"
"</tbody>\n"
"</tabla>\n"
"</td>\n"
"</tr>\n"
"\norte"
" <!-- PIE DE PIE -->\n"
"<tr>\n"
" <td align=\"center\" style=\"min-width: 590px;\">\n"
" <table width=\"590\" border=\"0\" cellpadding=\"0\" bgcolor=\"#875A7B\" style=\"min-width: 590px; color de fondo: rgb(135,90,123); relleno: 20px; border-collapse:separate;\">\n"
"<tr>\n"
" <td valign=\"middle\" align=\"left\" style=\"color: #fff; acolchado superior: 10px; parte inferior del relleno: 10px; tamaño de fuente: 12px;\">\n"
" ${company.name}<br/>\n"
" ${company.phone or ''}\n"
"</td>\n"
" <td valign=\"medio\" align=\"derecha\" style=\"color: #fff; acolchado superior: 10px; parte inferior del relleno: 10px; tamaño de fuente: 12px;\">\n"
" % si company.email:\n"
" <a href=\"mailto:${company.email}\" style=\"text-decoration:none; color: blanco;\">${company.email}</a><br/>\n"
" % endif\n"
" % si company.website web:\n"
" <a href=\"${company.website}\" style=\"text-decoration:none; color: blanco;\">\n"
" ${company.website web}\n"
"</a>\n"
" % endif\n"
"</td>\n"
"</tr>\n"
"</tabla>\n"
"</td>\n"
"</tr>\n"
"<tr>\n"
" <td align=\"centro\">\n"
" Impulsado por <a target=\"_blank\" href=\"https://www.odoo.com\">Odoo</a>.\n"
"</td>\n"
"</tr>\n"
"</tbody>\n"
"</tabla>\n"
"</cuerpo>\n"
"</html>\n"
" "
