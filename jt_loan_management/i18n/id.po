" ${perusahaan.telepon atau ''}\n"
" </td>\n"
" <td valign=\"middle\" align=\"right\" style=\"color: #fff; padding-top: 10px; padding-bottom: 10px; ukuran font: 12px;\">\n"
" % jika perusahaan.email:\n"
" <a href=\"mailto:${company.email}\" style=\"text-decoration:none; warna: putih;\">${perusahaan.email}</a><br/>\n"
"% endif\n"
" % jika perusahaan.situs web:\n"
" <a href=\"${company.website}\" style=\"text-decoration:none; warna: putih;\">\n"
" ${perusahaan.situs web}\n"
" </a>\n"
"% endif\n"
" </td>\n"
" </tr>\n"
" </tabel>\n"
" </td>\n"
" </tr>\n"
" <tr>\n"
" <td sejajar=\"pusat\">\n"
