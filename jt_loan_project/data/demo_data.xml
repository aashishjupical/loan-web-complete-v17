        <record id="project_loan_stage_0" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">Ekyc</field>
        </record>

        <record id="project_loan_stage_1" model="project.task.type">
            <field name="sequence">2</field>
            <field name="name">Submit Docs &amp; Fill Form</field>
        </record>

        <record id="project_loan_stage_3" model="project.task.type">
            <field name="sequence">4</field>
            <field name="name">To Approve Loan</field>
        </record>

        <record id="project_loan_stage_4" model="project.task.type">
            <field name="sequence">5</field>
            <field name="name">Schedule Atestation</field>
        </record>

        <record id="project_loan_stage_5" model="project.task.type">
            <field name="sequence">6</field>
            <field name="name">Esignature</field>
        </record>

        <record id="project_loan_stage_6" model="project.task.type">
            <field name="sequence">7</field>
            <field name="name">Loan Disbursement</field>
        </record>

        <record id="project_loan_stage_7" model="project.task.type">
            <field name="sequence">8</field>
            <field name="name">Terminated</field>
        </record>

        <record id="project_project_loan" model="project.project">
            <field name="date_start" eval="time.strftime('%Y-%m-01 10:00:00')"/>
            <field name="name">Loan Process</field>
            <field name="color">3</field>
            <field name="type_ids" eval="[(4, ref('project_loan_stage_0')),(4, ref('project_loan_stage_1')), (4, ref('project_loan_stage_3')), (4, ref('project_loan_stage_4')),(4, ref('project_loan_stage_5')), (4, ref('project_loan_stage_6')), (4, ref('project_loan_stage_7'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>

        <record id="terminate_reason_01" model="terminate.reason">
            <field name="name">Rejected By Applicant</field>
        </record>

        <record id="terminate_reason_02" model="terminate.reason">
            <field name="name">Time Limit Expired</field>
        </record>

