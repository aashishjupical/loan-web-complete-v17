            <field name="sequence" type="int">21</field>
            <field name="parent_id" search="[('parent_id','=',False),('website_id','=', 1)]"/>
            <field name="website_id" ref="website.default_website"/>
        </record>

        <record id="product_page_id" model="website.page">
            <field name="url">/product_page</field>
            <field name="is_published">True</field>
            <field name="view_id" ref="product_page"/>
            <field name="website_id" ref="website.default_website"/>
        </record>
