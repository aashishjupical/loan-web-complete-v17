<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="tenure_month_view_form" model="ir.ui.view">
        <field name="name">tenure.month.view.form</field>
        <field name="model">loan.month</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="tenure_month_view_tree" model="ir.ui.view">
        <field name="name">tenure.month.view.tree</field>
        <field name="model">loan.month</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_tenure_month">
        <field name="name">Tenure Month</field>
        <field name="res_model">loan.month</field>
        <field name="view_mode">tree,form</field>
    </record>

	<menuitem id="tenure_month_menu" name="Tenure Month" action="action_tenure_month" parent="project.menu_project_config"/> 
</odoo>